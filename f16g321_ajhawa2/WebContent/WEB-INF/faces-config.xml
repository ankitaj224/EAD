<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>bmiBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.BmiBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>weight</property-name>
			<property-class>double</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
			<property-name>height</property-name>
			<property-class>double</property-class>
			<value>0.0</value>
		</managed-property>
		<managed-property>
			<property-name>units</property-name>
			<property-class>java.lang.String</property-class>
			<value>English</value>
		</managed-property>
		<managed-property>
			<property-name>bmi</property-name>
			<property-class>double</property-class>
			<value>0.0</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionBeanLoan</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.ActionBeanLoan</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>loanBean</property-name>
			<property-class>edu.uic.ids.model.LoanBean</property-class>
			<value>#{loanBean}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/dataImport.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>

			<to-view-id>/dataImport.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>Failure</from-outcome>
			<to-view-id>/dataImport.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<managed-bean>
		<managed-bean-name>dbaseBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.DbaseBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>userBean</property-name>
			<property-class>edu.uic.ids.model.UserBean</property-class>
			<value>#{userBean}</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/databaseLogin.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>

			<to-view-id>/databaseView.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>Failure</from-outcome>
			<to-view-id>/databaseLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<managed-bean>
		<managed-bean-name>messageBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.MessageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionBeanFile</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.ActionBeanFile</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>messageBean</property-name>
			<property-class>edu.uic.ids.model.MessageBean</property-class>
			<value>#{messageBean}</value>
		</managed-property>
	</managed-bean>
	
<managed-bean>
		<managed-bean-name>actionStudentBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.ActionStudentBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionTestBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.ActionTestBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>question</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.Question</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/test.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>

			<to-view-id>/feedback.jsp</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>Failure</from-outcome>
			<to-view-id>/error.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
